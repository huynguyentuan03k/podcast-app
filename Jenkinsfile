pipeline {
    agent any
    stages {
        stage('Check Source Code') {
            steps {
                sh 'ls -al'
            }
        }
        stage("where i am here ?"){
            steps{
                sh 'pwd'
            }
        }
        stage("log everything!!!"){
            steps{
                sh "echo 'hello world' "
            }
        }
    }
}
